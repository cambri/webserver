[{{ website.id }}-{{ website.present().urlName }}]
listen=127.0.0.1:{{ 12000 + website.id }}
user={{ user }}
group={{ group }}

pm=dynamic
pm.max_children         = 20
pm.start_servers        = 5
pm.min_spare_servers    = 5
pm.max_spare_servers    = 10
pm.max_requests         = 20

chdir                   = {{ base_path }}